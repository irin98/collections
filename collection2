package com.ust.examples;

public class Collection2 {

	public static void main(String[] args) {
		int i=5;
		Integer obj=new Integer(i);
		int j=obj.intValue();
		System.out.println(j);
		
		double k=5.8;
		Double obj1=new Double(i);
		double h=obj1.doubleValue();
		System.out.println(h);
		

	}

}
